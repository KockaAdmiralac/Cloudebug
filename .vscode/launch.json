{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run Extension",
            "type": "extensionHost",
            "request": "launch",
            "args": [
                "--extensionDevelopmentPath=${workspaceFolder}/code/vscode"
            ],
            "outFiles": [
                "${workspaceFolder}/code/vscode/out/*.js",
                "${workspaceFolder}/code/vscode/out/**/*.js"
            ],
            "preLaunchTask": "${defaultBuildTask}"
        }
    ]
}
